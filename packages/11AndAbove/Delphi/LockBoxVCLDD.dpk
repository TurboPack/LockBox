{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DEFINE RELEASE}
{$ENDIF IMPLICITBUILDING}
{$DESCRIPTION 'TurboPack LockBox Delphi Vcl designtime package'}
{$LIBSUFFIX AUTO}
{$DESIGNONLY}
{$IMPLICITBUILD OFF}

requires
  vcl,
  designide,
  vclx,
  LockBoxDR;

contains
  Vcl.LbDesign in '..\..\..\source\Vcl.LbDesign.pas' {LbAboutForm},
  Vcl.LbKeyEd1 in '..\..\..\source\Vcl.LbKeyEd1.pas' {frmSymmetricKey},
  Vcl.LbKeyEd2 in '..\..\..\source\Vcl.LbKeyEd2.pas' {frmRSAKeys};

